	<parameter name="asignatura_id" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    (SELECT
            COUNT(evaluacion_asignatura.`q1_p1_nota`)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS total,
    (SELECT
            COUNT(evaluacion_asignatura.`q1_p1_nota`)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}
                and evaluacion_asignatura.`q1_p1_nota` >= 9) AS total_dar,
    (SELECT
            COUNT(evaluacion_asignatura.`q1_p1_nota`)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}
                and evaluacion_asignatura.`q1_p1_nota` >= 7
                AND evaluacion_asignatura.`q1_p1_nota` <= 8.99) AS total_aar,
    (SELECT
            COUNT(evaluacion_asignatura.`q1_p1_nota`)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}
                and evaluacion_asignatura.`q1_p1_nota` >= 5
                AND evaluacion_asignatura.`q1_p1_nota` <= 6.99) AS total_par,
    (SELECT
            COUNT(evaluacion_asignatura.`q1_p1_nota`)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}
                and evaluacion_asignatura.`q1_p1_nota` <= 4.99) AS total_naar,
    (SELECT
            ROUND (AVG(evaluacion_asignatura.`q1_p1_i1`),2)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS q1_p1_i1,
    (SELECT
            ROUND (AVG(evaluacion_asignatura.`q1_p1_i2`),2)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS q1_p1_i2,
    (SELECT
            ROUND (AVG(evaluacion_asignatura.`q1_p1_i3`),2)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS q1_p1_i3,
    (SELECT
            ROUND (AVG(evaluacion_asignatura.`q1_p1_i4`),2)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS q1_p1_i4,
    (SELECT
            ROUND (AVG(evaluacion_asignatura.`q1_p1_nota`),2)
        FROM
            `asignatura_dictada` asignatura_dictada
                INNER JOIN
            `evaluacion_asignatura_2016_julio` evaluacion_asignatura ON asignatura_dictada.`id` = evaluacion_asignatura.`asignatura_id`
        WHERE
            asignatura_dictada.`id` = $P{asignatura_id}) AS promedio_parcial;]]>
	</queryString>
